<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
    <!--别名-->
    <servlet-name>addServlet</servlet-name>
    <!--servlet所在的类-->
    <servlet-class>com.cms.controller.AddServlet</servlet-class>

    </servlet>

    <servlet-mapping>
        <!--别名-->
        <servlet-name>addServlet</servlet-name>
        <!--资源名-->
        <url-pattern>/main/java/com/controller/AddServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.cms.controller.Login</servlet-class>
    </servlet>

    <servlet-mapping>
        <!--别名-->
        <servlet-name>login</servlet-name>
        <!--资源名-->
        <url-pattern>/main/java/com/controller/Login</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>com.cms.controller.AdminServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/adminJsp</url-pattern>
    </servlet-mapping>
</web-app>